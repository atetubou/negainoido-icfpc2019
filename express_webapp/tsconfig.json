{
    "compilerOptions": {
        "outDir": "./dist",
        "sourceMap": true,
        "module": "commonjs",
        "target": "es6",
        "moduleResolution": "node",
        "removeComments": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
    },
    "include": [
        "./src/**/*",
        "node_modules/@types"
    ],
    "exclude": [ "node_modules "],
}
